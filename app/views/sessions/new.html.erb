<% provide(:title, "Log in") %>
<h1>Log In</h1>
<%= form_for(:session, url: login_path) do |f| %>
	<%= f.label :username %>
	<%= f.text_field :username %>

	<%= f.label :password %>
	<%= f.password_field :password %>

	<%= f.submit "Log in", class: "button" %>
<% end %>

<p>New user? <%= link_to "Sign up now." , signup_path %> </p>

